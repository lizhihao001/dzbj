/* pages/order/user.wxss */
page{
  background: #f7f7f7;
}
.header{
  width: 100%;
  height: 400rpx;
  background: #e6454a;
  text-align: center;
}
.header .user-header{
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  margin: 100rpx auto 0;
}
.header .user-name{
  text-align: center;
  color: #fff;
  font-size: 28rpx;
}
.header button{
  background-color: #e6454a;
}
.navlist{
  padding: 0;
}
.navlist .row{
  display: flex;
  align-items: center;
  padding: 30rpx;
  justify-content: space-between;
  background: #fff;
}
.navlist .row .item{
  display: flex;
  align-items: center;
  justify-content: space-betwee
}
.navlist .row .item .icon{
  margin-right: 20rpx;
}
.navlist .row .icon{
  width: 40rpx;
  height: 40rpx;
}
